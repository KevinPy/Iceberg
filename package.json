{
  "name": "iceberg-bp",
  "description": "Front-End Starter for your Web Projects with React, Webpack, Babel and SASS",
  "version": "3.0.0",
  "author": "Kevin Py",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:KevinPy/Iceberg.git"
  },
  "bugs": {
    "url": "https://github.com/KevinPy/Iceberg/issues"
  },
  "dependencies": {
    "history": "^4.7.2",
    "node-sass": "^4.7.2",
    "normalize.css": "^8.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.3",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "webpack-contrib/html-webpack-plugin",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.2.0",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "build": "webpack -p --mode development",
    "build:production": "webpack -p --mode production",
    "start": "webpack-dev-server --mode development",
    "test": "jest --setupTestFrameworkScriptFile=raf/polyfill",
    "test:ci": "jest --setupTestFrameworkScriptFile=raf/polyfill --ci"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/app/js/fileTransform.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testURL": "http://localhost:8080"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  }
}
